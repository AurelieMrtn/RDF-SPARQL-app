<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/aurel/ontologies/2023/2/untitled-ontology-48"
         xmlns="http://www.semanticweb.org/aurel/ontologies/2023/2/untitled-ontology-48#">

<owl:Ontology rdf:about="http://www.semanticweb.org/aurel/ontologies/2023/2/untitled-ontology-48"/>

<owl:ObjectProperty rdf:about="#hasGenre">
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="#Genre"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isWriterOf">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <rdfs:domain rdf:resource="#Writer"/>
  <rdfs:range rdf:resource="#Movie"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDirector">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <owl:inverseOf rdf:resource="#isDirectorOf"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="#Director"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasActor">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <owl:inverseOf rdf:resource="#isActorOf"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="#Actor"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isActorOf">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <rdfs:domain rdf:resource="#Actor"/>
  <rdfs:range rdf:resource="#Movie"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasWriter">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <owl:inverseOf rdf:resource="#isWriterOf"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="#Writer"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isDirectorOf">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <rdfs:domain rdf:resource="#Director"/>
  <rdfs:range rdf:resource="#Movie"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#Gender">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Language">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Age">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Location">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Theater"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Nationality">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Country">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Name">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Title">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#Year">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  <rdfs:domain rdf:resource="#Movie"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Genre">
  <rdfs:subClassOf rdf:resource="#Movie"/>
</owl:Class>

<owl:Class rdf:about="#Theater"/>

<owl:Class rdf:about="#Action">
  <rdfs:subClassOf rdf:resource="#Genre"/>
</owl:Class>

<owl:Class rdf:about="#Thriller">
  <rdfs:subClassOf rdf:resource="#Genre"/>
</owl:Class>

<owl:Class rdf:about="#Drama">
  <rdfs:subClassOf rdf:resource="#Genre"/>
</owl:Class>

<owl:Class rdf:about="#Crime">
  <rdfs:subClassOf rdf:resource="#Genre"/>
</owl:Class>

<owl:Class rdf:about="#Director">
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isDirectorOf"/>
      <owl:onClass rdf:resource="#Movie"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#Writer">
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isWriterOf"/>
      <owl:onClass rdf:resource="#Movie"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#Person"/>

<owl:Class rdf:about="#Movie">
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasGenre"/>
      <owl:onClass rdf:resource="#Genre"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Comedy">
  <rdfs:subClassOf rdf:resource="#Genre"/>
</owl:Class>

<owl:Class rdf:about="#Actor">
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isActorOf"/>
      <owl:onClass rdf:resource="#Movie"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Movie"/>
    <rdf:Description rdf:about="#Person"/>
    <rdf:Description rdf:about="#Theater"/>
  </owl:members>
</owl:AllDisjointClasses>


</rdf:RDF>
